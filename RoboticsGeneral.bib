@INPROCEEDINGS{5640506,
  author = {Srivastava, S. and Nandi, G.C.},
  title = {Localization of mobile robots in a network using mobile agents},
  booktitle = {Computer and Communication Technology (ICCCT), 2010 International
	Conference on},
  year = {2010},
  pages = {415 -420},
  month = sept.,
  abstract = {With the progressive growth in the domain of autonomous mobile robot,
	mobile robots should have the capability of taking intelligent decision
	for their movement plan. In real time, it should be necessary for
	mobile robots to consume less amount of energy in navigating also.
	In our paper we proposed a multi agent based architecture which aims
	to utilize mobile agent architecture and distributed computing for
	localization of mobile robots in a low bandwidth network. It involves
	information retrieval from the mobile robots, the information being
	their location in a global map. According to location information
	of each mobile robot, administrator implements low level control
	over mobile robots in their navigation plan and obstacle avoidance
	with the help of communication through mobile agents. This can also
	be done using basic client server architecture, but due to the inherent
	low bandwidth nature of the network, such architectures suffer from
	extreme lagging and network congestion as the number of networks'
	nodes increases, thus making it unsuitable for dynamic exchange of
	information. Hence we are using mobile agent architecture which involves
	transfer of executable code to the location of the data instead of
	transferring data to the location of the executable code.},
  annote = {another agent approach with JADE. one scenario for localization of
	all robots. interesting
	
	2011-01-23 Webot simulator reference a kind of find and collect example
	(Chia-HowLin [11]) reference other agent architecture approach: agent
	per behaviour webot as simulator a robot gui (similar to my stage
	approach) but with setting speed not position of simulated robot.
	
	},
  bdsk-file-1 = {},
  bdsk-url-1 = {http://dx.doi.org/10.1109/ICCCT.2010.5640506},
  date-added = {2011-01-05 13:09:48 +0100},
  date-modified = {2011-01-23 18:43:22 +0100},
  doi = {10.1109/ICCCT.2010.5640506},
  keywords = {autonomous mobile robot;client server architecture;distributed computing;dynamic
	information exchange;information retrieval;intelligent decision;mobile
	agent architecture;mobile robots localization;multi-agent based architecture;obstacle
	avoidance;client-server systems;collision avoidance;distributed processing;information
	retrieval;mobile agents;mobile robots;multi-agent systems;}
}

