	Doi = {10.1109/ICCCT.2010.5640506},
	Keywords = {autonomous mobile robot;client server architecture;distributed computing;dynamic information exchange;information retrieval;intelligent decision;mobile agent architecture;mobile robots localization;multi-agent based architecture;obstacle avoidance;client-server systems;collision avoidance;distributed processing;information retrieval;mobile agents;mobile robots;multi-agent systems;},
	Month = sept.,
