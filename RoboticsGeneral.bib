@INPROCEEDINGS{5556856,
  author = {Ahmed, M. and Khan, M.R. and Billah, M.M. and Farhana, S.},
  title = {A collaborative navigation algorithm for multi-agent robots in autonomous
	reconnaissance mission},
  booktitle = {Computer and Communication Engineering (ICCCE), 2010 International
	Conference on},
  year = {2010},
  pages = {1 -6},
  month = may,
  abstract = {Robots are playing an increasingly more important role as the technology
	is maturing. Robots are omnipresent; numerous robots committed to
	a single task will become ordinary. Multi-agent robots are capable
	of handling problems in many different ways than individuals while
	achieving higher levels of performance. But present unique challenges
	for developing collaborative navigation algorithm and coordination
	among themselves are not sufficient for autonomous collaborative
	tasks. Some research papers based on collaborative robots have been
	reviewed in this paper; from review analysis we have developed an
	algorithm with behavioral based distributed knowledge of multi-agents
	for accomplishing collaborative navigation. The goal of multi-agent
	robots is to move as a cohesive team by sharing their distributed
	learning behaviors and navigating in autonomous reconnaissance missions.
	Finally some preliminary experimental results will be presented of
	the developed collaborative navigation algorithm between robots to
	reach the target and achieve their goal sharing their knowledge.},
  annote = {architecture and example of the navigation(exploration?) sceneario
	with multi-agents
	
	2011-01-23 behaviour based and communication based reactive bahaviour
	and additive behavior result and subsumption neighborhood communication
	via bluetooth
	
	Scenario: Robot formations (synchronous robot behaviour)
	
	},
  bdsk-file-1 = {},
  bdsk-url-1 = {http://dx.doi.org/10.1109/ICCCE.2010.5556856},
  date-added = {2011-01-05 13:17:32 +0100},
  date-modified = {2011-01-23 18:06:57 +0100},
  doi = {10.1109/ICCCE.2010.5556856},
  keywords = {autonomous collaborative tasks;autonomous reconnaissance mission;cohesive
	team;collaborative navigation algorithm;coordination;distributed
	knowledge;distributed learning behaviors;goal sharing;multiagent
	robots;control engineering computing;learning (artificial intelligence);multi-agent
	systems;multi-robot systems;navigation;path planning;}
}

