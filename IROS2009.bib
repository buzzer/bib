	Keywords = {boundary detection algorithm;distributed boundary detection algorithm;dynamic network topologies;exterior boundary;interior void boundary;local boundary classifications;local network geometry;multi robot systems;rapidly changing networks;robots network connectivity;robust boundary subgraph;algorithm theory;multi-robot systems;network topology;telecommunication networks;},
	Month = oct.,
	Pages = {4791 -4798},
	Title = {A Distributed boundary detection algorithm for multi-robot systems},
