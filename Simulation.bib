  bib2html_pubtype = {Conference Paper},
  bib2html_rescat = {Planning},
  date-modified = {2014-05-02 13:12:51 +0000},
  owner = {Sebastian Rockel},
  timestamp = {2013.06.04},
  url = {https://ias.cs.tum.edu/_media/spezial/bib/moesenle09icaps.pdf}
