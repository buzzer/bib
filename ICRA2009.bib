	Doi = {10.1109/ROBOT.2009.5152829},
	Issn = {1050-4729},
	Keywords = {Brick amp;Mortar algorithm;LVD algorithm;ant algorithm;local Voronoi decomposition algorithm;multiagent task allocation;multirobot system;online task allocation;robust control;mobile robots;multi-agent systems;multi-robot systems;robust control;},
	Month = may,
	Pages = {1935 -1940},
	Title = {Local Voronoi Decomposition for multi-agent task allocation},
	Year = {2009},
